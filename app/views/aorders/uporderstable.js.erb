$('#olist_div').html("<%= escape_javascript render(:partial => 'olist', :object=>@aorders) %>")
$('#onlinedrivers').html("<%= escape_javascript render(:partial => 'onlinedrivers', :object=>@onlineauto) %>")


$(document).ready(

  $( ".hoverable").hover(

    function () {
      var hoveredorderids = $(this).data('orderid')
      $(".hoverable").each(function(){
        var $i = $( this )
        try{
          $i.data('orderid').forEach(function(d){
            if ($.inArray(d, hoveredorderids ) != -1 ) {
              $i.addClass("highlighted");
            }
          })
        }
        catch(e){}
      })
    },

    function () {
      $(".hoverable").removeClass("highlighted");
    }

  )


);